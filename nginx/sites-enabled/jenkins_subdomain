upstream jenkins_server {
    server 127.0.0.1:8080
}

server {

    listen *:80;
    #server_name jenkins.*;
    server_name *;

    access_log  /var/log/nginx/jenkins-proxy.log;
    error_log   /var/log/nginx/jenkins-proxy.error.log;

    location / {
      proxy_pass http://jenkins_server;
    }
  }